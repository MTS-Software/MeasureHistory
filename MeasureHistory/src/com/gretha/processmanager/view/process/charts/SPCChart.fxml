<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.AreaChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gretha.processmanager.view.process.charts.SPCChartController">
   <center>
      <AreaChart fx:id="chart" createSymbols="false" horizontalZeroLineVisible="false" style="-fx-background-color: white;" verticalZeroLineVisible="false" BorderPane.alignment="CENTER">
        <xAxis>
          <NumberAxis fx:id="chartxAxis" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="chartyAxis" side="LEFT" />
        </yAxis>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </AreaChart>
   </center>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
