<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gretha.processmanager.view.process.result.ResultTableController">
	<center>
		<TableView fx:id="tblResult" tableMenuButtonVisible="true" BorderPane.alignment="CENTER">
			<columns>
				<TableColumn fx:id="colNr" minWidth="40.0" prefWidth="40.0" text="Nr." />
				<TableColumn fx:id="colValue" minWidth="115.0" prefWidth="115.0" text="Messwert" />
				<TableColumn fx:id="colLoLim" minWidth="115.0" prefWidth="115.0" text="Untergrenze" />
				<TableColumn fx:id="colUpLim" minWidth="115.0" prefWidth="115.0" text="Obergrenze" />
				<TableColumn fx:id="colState" minWidth="115.0" prefWidth="115.0" text="Status" />
				<TableColumn fx:id="colSerial" minWidth="120.0" prefWidth="120.0" text="Seriennummer" />
				<TableColumn fx:id="colTypNr" minWidth="115.0" prefWidth="115.0" text="Typen-Nummer" />
				<TableColumn fx:id="colWtNr" minWidth="115.0" prefWidth="115.0" text="WT-Nummer" />
				<TableColumn fx:id="colRemark" minWidth="200.0" prefWidth="200.0" text="Bemerkung" />
				<TableColumn fx:id="colTimestamp" minWidth="120.0" prefWidth="120.0" text="Zeitpunkt" />
			</columns>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
		</TableView>
	</center>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
